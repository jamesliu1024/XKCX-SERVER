<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="seig.ljm.xkckserver.mapper.AccessLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="seig.ljm.xkckserver.entity.AccessLog">
        <id column="log_id" property="logId" />
        <result column="visitor_id" property="visitorId" />
        <result column="device_id" property="deviceId" />
        <result column="access_time" property="accessTime" />
        <result column="access_type" property="accessType" />
        <result column="result" property="result" />
        <result column="reason" property="reason" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        log_id, visitor_id, device_id, access_time, access_type, result, reason
    </sql>

</mapper>
